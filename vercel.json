{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/leads.html",
      "dest": "/leads.html"
    },
    {
      "src": "/leads/?",
      "dest": "/leads.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/dashboard.html"
    },
    {
      "src": "/dashboard/?",
      "dest": "/dashboard.html"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/scraper/?",
      "dest": "/index.html"
    },
    {
      "src": "/email-templates.html",
      "dest": "/email-templates.html"
    },
    {
      "src": "/email-templates/?",
      "dest": "/email-templates.html"
    },
    {
      "src": "/settings.html",
      "dest": "/settings.html"
    },
    {
      "src": "/settings/?", 
      "dest": "/settings.html"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|html))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard.html"
    }
  ]
} 